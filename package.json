{
  "name": "hexo-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "hexo generate",
    "clean": "hexo clean",
    "server": "hexo server",
    "new": "node ./shell/new.js",
    "deploy": "node ./shell/deploy.js --bucket=$bucket --region=$region --accessKeyId=$accessKeyId --accessKeySecret=$accessKeySecret",
    "server:debug": "hexo server --debug",
    "commit": "git pull && git add . && git commit -m 'commit' && git push"
  },
  "hexo": {
    "version": "5.3.0"
  },
  "dependencies": {
    "ali-oss": "^6.13.2",
    "hexo": "^5.0.0",
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-category": "^1.0.0",
    "hexo-generator-index": "^2.0.0",
    "hexo-generator-searchdb": "^1.3.3",
    "hexo-generator-tag": "^1.0.0",
    "hexo-renderer-ejs": "^1.0.0",
    "hexo-renderer-marked": "^3.0.0",
    "hexo-renderer-stylus": "^2.0.0",
    "hexo-renderer-swig": "^1.1.0",
    "hexo-server": "^2.0.0",
    "hexo-theme-landscape": "^0.0.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "minimist": "^1.2.5"
  }
}
